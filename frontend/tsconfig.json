{
    "compilerOptions": {
        "target": "ES2022",
        "lib": [
            "ES2023",
            "DOM",
            "DOM.Iterable"
        ],
        "allowJs": false,
        "skipLibCheck": true,
        "strict": true,
        "noEmit": true,
        "esModuleInterop": true,
        "module": "ESNext",
        "moduleResolution": "Bundler",
        "resolveJsonModule": true,
        "isolatedModules": true,
        "jsx": "preserve",
        "forceConsistentCasingInFileNames": true,
        "incremental": true
    },
    "include": [
        "next-env.d.ts",
        "src"
    ],
    "exclude": [
        "node_modules"
    ]
}